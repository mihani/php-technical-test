<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}
    </head>
    <body class="sb-nav-fixed">
        {% include '_partials/nav/topnav.html.twig' %}
        <div id="layoutSidenav">
            <div id="layoutSidenav_nav">
                {% include '_partials/nav/sidenav.html.twig' %}
            </div>
            <div id="layoutSidenav_content">
                <main>
                    <div class="container-fluid">
                        {% block body %}{% endblock %}
                    </div>
                </main>
            </div>
        </div>
        {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
        {% endblock %}
    </body>
</html>
